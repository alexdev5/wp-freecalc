{

  " имя " : " mpdf / mpdf " ,
  " тип " : " библиотека " ,

  " description " : " Библиотека PHP, генерирующая файлы PDF из HTML в кодировке UTF-8 " ,

  " ключевые слова " : [ " php " , " pdf " , " utf-8 " ],
  " домашняя страница " : " https://mpdf.github.io " ,

  " лицензия " : [ "только GPL-2.0 " ],

  " авторы " : [
    {
      " name " : " Matěj Humpál " ,
      " role " : " Разработчик, сопровождающий "
    },
    {
      " name " : " Ян Бэк " ,
      " role " : " Разработчик (на пенсии) "
    }
  ],

  " support " : {

    " issues " : " https://github.com/mpdf/mpdf/issues " ,
    " источник " : " https://github.com/mpdf/mpdf " ,
    " документы " : " http://mpdf.github.io "

  },

  " require " : {

    " php " : " ^ 5.6 || ^ 7.0 || ~ 8.0.0 " ,

    " ext-gd " : " * " ,
    " ext-mbstring " : " * " ,

    " psr / log " : " ^ 1.0 " ,
    " setasign / fpdi " : " ^ 2.1 " ,
    " paragonie / random_compat " : " ^ 1.4 | ^ 2.0 | ^ 9.99.99 " ,
    " myclabs / deep-copy " : " ^ 1.7 "

  },

  " require-dev " : {

    " phpunit / phpunit " : " ^ 5.7 " ,
    " издевательство / издевательство " : " ^ 1.3.0 " ,
    " squizlabs / php_codesniffer " : " ^ 3.5.0 " ,
    " tracy / tracy " : " ^ 2.4 " ,
    " mpdf / qrcode " : " ^ 1.1.0 "

  },

  " подсказывать " : {

    " ext-bcmath " : " Требуется для генерации некоторых типов штрих-кодов " ,
    " ext-zlib " : " Требуется для сжатия встроенных ресурсов, например шрифтов " ,
    " ext-xml " : " Требуется в основном для работы с SVG "

  },

  " autoload " : {

    " пср-4 " : {
      " Mpdf \\ " : " src / "
    }

  },

  " autoload-dev " : {

    " пср-4 " : {
      " Mpdf \\ " : " тесты / Mpdf "
    },

    " файлы " : [
      " src / functions-dev.php "
    ]

  },

  " scripts " : {
    " post-install-cmd " : [
      " php -r \" chmod ('./ tmp', 0777); \ " "
    ],
    " cs " : " @php vendor / bin / phpcs -v --report-width = 160 --standard = ruleset.xml --severity = 1 --warning-severity = 0 --extensions = php src utils tests " ,
    " test " : " @php vendor / bin / phpunit " ,
    " покрытие " : " @php vendor / bin / phpunit --coverage-text "
  }

}